CC		=	gcc	-o

SRC=	main.c

CFLAGS	=	-Wall	-Wextra	-pedantic	--std=c99 -g

NAME	=	countwords

all:
	$(CC)	$(NAME)	$(SRC)	$(CFLAGS)

clean:

fclean: clean
	rm	-f	$(NAME)

re:	fclean

.PHONY:	all	clean	fclean	re
